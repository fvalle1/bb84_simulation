#include <TObject.h>
#include <TTree.h>
#include "TPhone.h"
#include "TQbit.h"

ClassImp(TPhone)

//constructor
TPhone::TPhone()
{
}

//destructor
TPhone::~TPhone()
{
}

//SetQbitA
void TPhone::SetQbitA(TQbit* qbitA)
{
  //saving qbitA generated by Alice
  fQbitA = new TQbit(*qbitA);
}

//Fill
TPhone::Fill(TQbit qbitB)
{
  //saving qbitB generated by Bob
  //checking if qbitA == qbitB, if baseA == baseB ecc...
  fQbitB = qbitB;
  This->Fill();  
}
