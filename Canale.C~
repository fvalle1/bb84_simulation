#ifndef TCHANEL_H
#define TCHANEL_H
 
#include <TRandom3.h>
#include <TMath.h>

class TChanel : public TObject {
  
 private:
  double fQbit;
  double fNoise;
    
 public:
  TChanel(TQbit fQbit);
  virtual ~TChanel();
  TChannel AddNoise(double theta, TF1 *PdfNoise);
  virtual Get(double a, double b);
  
  ClassDef(TProva,1)  
};

#endif
